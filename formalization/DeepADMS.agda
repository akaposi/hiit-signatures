-- Non-inductive functions
--------------------------------------------------------------------------------

ΠNI : ∀ {Γ}(A : Set) → (A → Ty Γ) → Ty Γ
ΠNI {Γᴬ , Γᴰ , Γᴹ , Γˢ} A B =
  (λ γ            → (α : A) → B α .₁ γ) ,
  (λ γ γᴰ f       → (α : A) → B α .₂ .₁ γ γᴰ (f α)) ,
  (λ γ₀ γ₁ γᴹ f₀ f₁ → (α : A) → B α .₂ .₂ .₁ _ _ γᴹ (f₀ α) (f₁ α)) ,
  (λ γ γᴰ γˢ f fᴰ → (α : A) → B α .₂ .₂ .₂ γ γᴰ γˢ (f α) (fᴰ α))

ΠNI[] : ∀ {Γ Δ}{σ : Sub Γ Δ}{A : Set}{B : A → Ty Δ}
